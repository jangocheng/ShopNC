@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <link href="~/Scripts/jquery-easyui-1.4.3/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Scripts/jquery-easyui-1.4.3/themes/icon.css" rel="stylesheet" />
    <link href="~/Content/Admin/WHS/style.css" rel="stylesheet" />
    <link href="~/Content/Admin/WHS/default.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/Index.css" rel="stylesheet" />
    <style type="text/css">
        #header {
            width: 100%;
            overflow: hidden;
            background: url('/Content/Admin/WHS/master_header.png') #B3DFDA no-repeat left center 0 0;
        }

        .panel-body {
            padding: 0;
        }

        .panel {
            margin-bottom: 2px;
        }

        .easyui-accordion > div {
            height: auto;
            border-bottom: 0;
        }

        .panel-title {
            font-size: 13px;
            font-weight: bold;
            color: #0e2d5f;
            height: 16px;
            line-height: 16px;
        }

        span.nav {
            display: inline-block;
        }

        #nav ul li {
            border: 1px dashed #fff;
        }

            #nav ul li:hover {
                border: 1px dashed #b7daf5;
                background: #e1eff9;
            }

            #nav ul li a {
                display: inline-block;
                width: 100%;
            }
    </style>
</head>

<body class="easyui-layout">
    <!--------------头部---------------------------->
    <div class="header" id="header" data-options="region:'north'">
        <div class="navmenu" style="display:none;">
            <ul class="menu">
                <li class="current" id="menu0"><a href="/"><span><img src="/Content/icon/product.png" height="15px" />商 品</span></a> </li>

                <li class="" id="menu1"><a href="/"><span><img src="/Content/icon/order.png" height="15px" />订 单</span></a> </li>

                <li class="" id="menu2"><a href="/"><span><img src="/Content/icon/user.png" height="15px" />会 员</span></a> </li>

                <li class="" id="menu3"><a href="/"><span><img src="/Content/icon/promotion.png" height="15px" />营 销</span></a> </li>

                <li class="" id="menu4"><a href="/"><span><img src="/Content/icon/theme.png" height="15px" />页 面</span></a> </li>

                <li class="" id="menu5"><a href="/"><span><img src="/Content/icon/config.png" height="15px" />配 置</span></a> </li>

                <li class="" id="menu6"><a href="/"><span><img src="/Content/icon/value.png" height="15px" />增 值</span></a> </li>

                <li class="" id="menu7"><a href="/"><span><img src="/Content/icon/tool.png" height="15px" />工具箱</span></a> </li>

            </ul>
            <ul class="tool"><li><span><a href="#">桌面</a> | <a href="javascript:void(0);" onclick="Copyright();">关于</a></span></li></ul><ul class="faq"></ul>
        </div>

        <div class="userinfo" style="display:block">
            <font color="red" size="5">@ViewBag.FullName</font>，您好！
        </div>
        <ul class="navigation" style="display:block"></ul>
        <ul class="button">
            <li style="margin-top:10px;">
                当前时间：<b id="date"></b>
            </li>
            <li>
                <a href="javascript:void(0)" onclick="javascript:ShowPasswordDialog()" title="修改用户密码">
                    <img border="0" src="~/Content/Admin/WHS/btn_hd_support.gif" title="修改用户密码" alt="修改用户密码" />
                </a>
            </li>
            <li>
                <a href="#" onclick="addTab('在线帮助', 'userGuid.htm', 'icon icon-help')" title="在线帮助">
                    <img border="0" src="~/Content/Admin/WHS/btn_hd_help.gif" title="在线帮助" alt="在线帮助" />
                </a>
            </li>
            <li>
                <a href="/Login/Index" title="注销"><b><img border="0" src="~/Content/Admin/WHS/btn_hd_exit.gif" title="注销" alt="注销" /></b></a>
            </li>
        </ul>
    </div>

    <!--左侧导航菜单-->
    <div region="west" split="true" title="导航菜单" style="width:200px;padding:1px;overflow:hidden;">
        <div id="nav" class="easyui-accordion" fit="true" border="false">
            <div title="用户管理" iconcls="icon-computer" style="padding:10px;overflow:auto;" selected="true">
                <ul>
                    <li class="menuItem" icon="icon-user" href="@Url.Action("Index", "UserInfo", new { Area="Admin"})"><div><a href="javascript:;"><span class="icon icon-user">&nbsp;</span><span class="nav">管理员管理</span></a></div></li>
                    <li class="menuItem" icon="icon-lock"><div><a href="javascript:;"><span class="icon icon-lock">&nbsp;</span><span class="nav">角色管理</span></a></div></li>
                    <li class="menuItem" icon="icon-lock"><div><a href="javascript:;"><span class="icon icon-lock">&nbsp;</span><span class="nav">权限组管理</span></a></div></li>
                    <li class="menuItem" icon="icon-lock"><div><a href="javascript:;"><span class="icon icon-lock">&nbsp;</span><span class="nav">权限管理</span></a></div></li>
                </ul>
            </div>
            <div title="基本配置" iconcls="icon-computer" style="padding:10px;overflow:hidden;height:1000px;" selected="true">
                <ul>
                    <li class="menuItem" icon="icon-lock"><div><a href="javascript:;"><span class="icon icon-lock">&nbsp;</span><span class="nav">地区</span></a></div></li>
                </ul>
            </div>
        </div>
    </div>

    <!--Tab上菜单操作-->
    <div id="mm" class="easyui-menu" style="width:150px;">
        <div id="mm-tabupdate">刷新</div>
        <div class="menu-sep"></div>
        <div id="mm-tabclose">关闭</div>
        <div id="mm-tabcloseall">全部关闭</div>
        <div id="mm-tabcloseother">除此之外全部关闭</div>
        <div class="menu-sep"></div>
        <div id="mm-tabcloseright">当前页右侧全部关闭</div>
        <div id="mm-tabcloseleft">当前页左侧全部关闭</div>
        <div class="menu-sep"></div>
        <div id="mm-exit">退出</div>
    </div>

    <!--主工作区-->
    <div id="mainPanle" region="center" title="" style="overflow:hidden;">
        <div id="tabs" class="easyui-tabs" fit="true">
            <div title="首页" data-options="icon:'icon-house'" closable="false">
                <iframe src="" frameborder="0" scrolling="no"></iframe>
            </div>
        </div>
    </div>


    <div class="footer" data-options="region:'south',split:true" style="height:30px;">
        版权所有：-----ShopNC
    </div>
    <div id='Loading' style="position: absolute; z-index: 1000; top: 0px; left: 0px; width: 100%; height: 100%; background: #fff url('/Content/Image/loading.jpg') no-repeat center ; text-align: center; padding-top: 20%;"><h1><image src='/Content/Image/loading.jpg' /><font color="#15428B"></font></h1></div>
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("/Scripts/jquery-easyui-1.4.3/jquery.easyui.min.js")
    <script src="~/Scripts/datapattern.js"></script>
    <script src="~/Scripts/Admin/Index.js"></script>
    <script type="text/javascript">
        function closes() {
            $("#Loading").fadeOut("normal", function () {
                $(this).remove();
            });
        }
        var pc;
        $.parser.onComplete = function () {
            if (pc) clearTimeout(pc);
            pc = setTimeout(closes, 1000);
        }

    </script>
</body>
</html>
